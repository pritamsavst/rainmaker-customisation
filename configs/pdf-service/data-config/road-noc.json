{

   "key":"road-noc",
   "DataConfigs": {
      "serviceName": "pm-services",
      "version": "1.0.0",
      "baseKeyPath": "$.nocApplicationDetail.*",
      "entityIdPath":"$.applicationId",
      "isCommonTableBorderRequired":true,
      "mappings": [
         {
            "mappings": [
               {
                  "direct": [
                     {
                        "variable": "variable1",
                        "value": {
                             "path":"$.approveDate"                              
                        }
                     },            
                     {
                        "variable":"variable2",
                        "value":{
